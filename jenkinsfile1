pipeline {

	agent any
			
	stages {
	
		stage ('clone') {
		   
		   steps {
			
				sh 'echo cloning my code'
		   }
		   
		}
		
		stage ('test') {
		
			steps {
			
				sh 'echo testing my code '
				
				}
				
			}

		stage ('build') {

			steps {
			
				sh 'echo building the job '
				
				}
				
			}	
		
		
	
	}		
}
